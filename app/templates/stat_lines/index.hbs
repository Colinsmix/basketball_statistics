<h3>Index</h3>

<p>{{link-to 'New stat_line' 'stat_lines.new'}}</p>

<table>
  <tr>
    <th>id</th>
    <th>Player</th>
    <th>Game</th>
    <th>Points</th>
  </tr>
  {{#each}}
    <tr>
      <td>{{link-to id 'stat_lines.show' this}}</td>
      <td>{{player_id}}</td>
      <td>{{game_id}}</td>
      <td>{{points}}</td>
    </tr>
  {{/each}}
</table>
